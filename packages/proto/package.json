{
  "name": "@dsstudio/proto",
  "version": "0.0.6",
  "keywords": [
    "design tokens",
    "scaffold component library",
    "design system studio"
  ],
  "homepage": "https://github.com/tehkaiyu/design-system-studio",
  "bugs": "https://github.com/tehkaiyu/design-system-studio/issues",
  "repository": "tehkaiyu/design-system-studio",
  "license": "MIT",
  "author": "Kaiyu Hsu @tehkaiyu",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "proto": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "prepack": "npm run build",
    "postpack": "rm -f oclif.manifest.json",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "cli-ux": "^5.5.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@types/node": "^14.14.10",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "proto",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "gitHead": "2167c9242e953f23674c6cf52bfaa5ef683cde38"
}
