  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/proto.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "dev": "npm start",
    "prebuild": "yarn icons",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "prebuild-storybook": "yarn icons",
    "build-storybook": "build-storybook --docs",
    "chromatic": "CHROMATIC_APP_CODE=$CHROMATIC_APP_CODE chromatic --exit-zero-on-changes",
    "icons": "svgr --template ./codegen/icon-template.js --index-template ./codegen/icon-index-template.js --ext tsx --icon --filename-case 'kebab' --title-prop --no-prettier --ref -d ./src/components/icons ./src/components/icons",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "react": ">=16",
    "styled-components": ">=5",
    "styled-system": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@dss/tokens": "^1.0.0",
    "@babel/core": "^7.8.4",
    "@storybook/addon-a11y": "^6.0.3",
    "@storybook/addon-actions": "^6.0.3",
    "@storybook/addon-contexts": "^5.3.19",
    "@storybook/addon-docs": "^6.0.3",
    "@storybook/addon-knobs": "^6.0.3",
    "@storybook/addon-links": "^6.0.3",
    "@storybook/addon-viewport": "^6.0.3",
    "@storybook/addons": "^6.0.3",
    "@storybook/react": "^6.0.3",
    "@svgr/cli": "^5.1.0",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.6",
    "babel-loader": "8.1.0",
    "camelcase": "^6.0.0",
    "husky": "^4.2.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "storybook-chromatic": "^4.0.2",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.4",
    "ts-loader": "^8.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.2"
