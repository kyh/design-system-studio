{
  "name": "@dsstudio/proto",
  "version": "0.0.12",
  "keywords": [
    "design tokens",
    "scaffold component library",
    "design system studio"
  ],
  "homepage": "https://github.com/tehkaiyu/design-system-studio",
  "bugs": "https://github.com/tehkaiyu/design-system-studio/issues",
  "repository": "tehkaiyu/design-system-studio",
  "license": "MIT",
  "author": "Kaiyu Hsu @tehkaiyu",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "proto": "./bin/run"
  },
  "files": [
    "/bin",
    "/build",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "npm run clean && tsc -b && oclif-dev manifest && oclif-dev readme",
    "clean": "rm -rf build",
    "prepare": "npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.5.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@types/node": "^14.14.10",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./build/commands",
    "bin": "proto",
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "gitHead": "c6e4b4d2ce46642e8b316bbfaa26007860a4b8a9"
}
